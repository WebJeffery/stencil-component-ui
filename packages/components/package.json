{
  "name": "swc-ui",
  "version": "0.0.1",
  "description": "一个使用stencilJs开发的web component通用组件库",
  "main": "dist/components/index.js",
  "type": "module",
  "module": "dist/components/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/components/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/WebJeffery/stencil-component-ui.git"
  },
  "files": [
    "dist/",
    "readme.md"
  ],
  "keywords": [
    "component",
    "web-component",
    "swc-ui",
    "ui"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "serve": "stencil build --dev --es5 --watch --serve",
    "start": "stencil build --prod --es5 --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^16.18.64",
    "classnames": "^2.5.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.5.2"
  },
  "license": "MIT",
  "private": false
}
