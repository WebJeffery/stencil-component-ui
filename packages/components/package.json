{
  "name": "@swc-ui/components",
  "version": "0.1.4",
  "description": "一个使用stencil.js开发的web component通用组件库",
  "main": "dist/cjs/loader.cjs.js",
  "module": "dist/esm/loader.js",
  "unpkg": "dist/esm/loader.js",
  "types": "dist/types/index.d.ts",
  "collection": "/dist/collection/collection-manifest.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/WebJeffery/stencil-component-ui.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "keywords": [
    "component",
    "web-component",
    "swc-ui",
    "ui"
  ],
  "scripts": {
    "start": "stencil build --prod --es5 --watch",
    "build": "stencil build --docs",
    "clean": "rimraf dist loader www docs",
    "watch": "npm run clean && stencil build --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.2",
    "@stencil/sass": "^3.0.11",
    "@types/jest": "^29.5.10",
    "@types/node": "^16.18.64",
    "classnames": "^2.5.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.5.2"
  },
  "license": "MIT",
  "dependencies": {
    "@swc-ui/icons": "workspace:^"
  },
  "publishConfig": {
    "access": "public"
  }
}
