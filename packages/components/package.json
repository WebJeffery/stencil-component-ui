{
  "name": "swc-ui",
  "version": "0.0.1",
  "description": "一个使用stencilJs开发的web component通用组件库",
  "main": "dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "./dist/types/types.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "unpkg": "dist/components/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/WebJeffery/stencil-component-ui.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "keywords": [
    "component",
    "web-component",
    "swc-ui",
    "ui"
  ],
  "scripts": {
    "clean": "rimraf dist loader www docs",
    "build": "npm run clean && stencil build --docs",
    "watch": "npm run clean && stencil build --watch",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.2",
    "@stencil/sass": "^3.0.11",
    "@types/jest": "^29.5.10",
    "@types/node": "^16.18.64",
    "classnames": "^2.5.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.5.2"
  },
  "license": "MIT",
  "private": false
}
